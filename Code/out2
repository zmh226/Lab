FUNCTION fact :
PARAM v1
t1 := v1
t2 := #1
IF t1 == t2 GOTO label1
GOTO label2
LABEL label1 :
RETURN v1
GOTO label3
LABEL label2 :
t4 := v1
t7 := v1
t8 := #1
t6 := t7 - t8
ARG (null)
t5 := CALL fact
t3 := t4 * t5
RETURN t3
LABEL label3 :

FUNCTION main :
READ t9
v2  :=  t9
t10 := v2
t11 := #1
IF t10 > t11 GOTO label4
GOTO label5
LABEL label4 :
t13 := v2
ARG (null)
t12 := CALL fact
v3  :=  t12
GOTO label6
LABEL label5 :
t14 := #1
v3  :=  t14
LABEL label6 :
t15 := v3
WRITE t15
RETURN #0

